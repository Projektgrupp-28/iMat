<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BorderPaneDemoController">
   <children>
      <BorderPane fx:id="mainPane" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <VBox BorderPane.alignment="CENTER">
               <children>
                  <VBox>
                     <children>
                        <HBox alignment="CENTER" prefHeight="80.0" spacing="40.0" style="-fx-background-color: black;">
                           <children>
                              <Label id="iMatLogo" alignment="CENTER_RIGHT" prefHeight="40.0" text="iMat" textAlignment="CENTER" textFill="#fffdfd" HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                              </Label>
                              <HBox alignment="CENTER" prefHeight="40.0" spacing="10.0" HBox.hgrow="ALWAYS">
                                 <children>
                                    <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="300.0" style="-fx-background-radius: 40; -fx-background-color: white;">
                                       <graphic>
                                          <HBox alignment="CENTER" spacing="10.0">
                                             <children>
                                                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                                   <HBox.margin>
                                                      <Insets left="20.0" />
                                                   </HBox.margin>
                                                </ImageView>
                                                <TextField prefHeight="40.0" promptText="Mjölk, banan, ..." HBox.hgrow="ALWAYS" />
                                             </children>
                                          </HBox>
                                       </graphic>
                                    </Button>
                                    <Button mnemonicParsing="false" prefHeight="40.0">
                                       <graphic>
                                          <HBox alignment="CENTER" spacing="5.0">
                                             <children>
                                                <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" />
                                                <Label text="Min profil" textFill="WHITE" />
                                             </children>
                                          </HBox>
                                       </graphic>
                                    </Button>
                                 </children>
                              </HBox>
                              <Button graphicTextGap="10.0" mnemonicParsing="false" prefHeight="40.0" style="-fx-background-radius: 40; -fx-background-color: white;">
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                                 <graphic>
                                    <HBox alignment="CENTER" spacing="5.0">
                                       <children>
                                          <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" />
                                          <HBox alignment="TOP_CENTER" spacing="2.0">
                                             <children>
                                                <Label styleClass="font-larger" text="8 888">
                                                   <HBox.margin>
                                                      <Insets top="3.0" />
                                                   </HBox.margin>
                                                </Label>
                                                <Label styleClass="font-normal" text="55">
                                                   <font>
                                                      <Font size="1.0" />
                                                   </font>
                                                   <HBox.margin>
                                                      <Insets top="1.0" />
                                                   </HBox.margin>
                                                </Label>
                                                <Label alignment="CENTER" contentDisplay="BOTTOM" styleClass="font-larger" text=":-">
                                                   <HBox.margin>
                                                      <Insets bottom="5.0" left="1.0" />
                                                   </HBox.margin>
                                                </Label>
                                             </children>
                                          </HBox>
                                       </children>
                                    </HBox>
                                 </graphic>
                              </Button>
                           </children>
                           <padding>
                              <Insets left="30.0" right="30.0" />
                           </padding>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="45.0" prefWidth="330.0" spacing="50.0">
                           <children>
                              <AnchorPane style="-fx-background-color: #C0FFF7;" HBox.hgrow="ALWAYS">
                                 <children>
                                    <HBox alignment="CENTER_LEFT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="0.0">
                                       <children>
                                          <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" style="-fx-background-color: transparent;" styleClass="font-normal" text="Inköpslistor">
                                             <graphic>
                                                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" />
                                             </graphic>
                                          </Button>
                                          <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" style="-fx-background-color: transparent;" styleClass="font-normal" text="Gillade varor">
                                             <graphic>
                                                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" />
                                             </graphic>
                                          </Button>
                                       </children>
                                    </HBox>
                                    <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="90.0" style="-fx-background-color: transparent;" styleClass="font-normal" text="Hjälp" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="0.0">
                                       <graphic>
                                          <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" />
                                       </graphic>
                                    </Button>
                                 </children>
                              </AnchorPane>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </VBox>
         </top>
         <left>
            <AnchorPane prefHeight="375.0" prefWidth="180.0" style="-fx-background-color: white;" BorderPane.alignment="CENTER">
               <children>
                  <VBox alignment="TOP_CENTER" prefHeight="375.0" prefWidth="180.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <Label style="-fx-background-color: white;" styleClass="font-larger" text="Katalog">
                           <VBox.margin>
                              <Insets top="30.0" />
                           </VBox.margin>
                        </Label>
                        <ListView fx:id="categoriesList" onMouseClicked="#displaySelectedFromList" prefHeight="250.0" prefWidth="180.0">
                           <VBox.margin>
                              <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                           </VBox.margin>
                        </ListView>
                        <Button mnemonicParsing="false" onAction="#handleSnacksAction" text="Button" />
                     </children>
                  </VBox>
               </children>
            </AnchorPane>
         </left>
      </BorderPane>
   </children>
</AnchorPane>
