<AnchorPane fx:id="shoppingCartLayerPane" onMouseClicked="#closeButtonPressed" style="-fx-background-color: rgba(0,0,0,0.75);" stylesheets="@../../styling/shoppingCartStyle.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.shoppingcart.ShoppingCartController">
   <children>
      <BorderPane fx:id="shoppingCartBorderPane" layoutX="186.0" layoutY="56.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onMouseClicked="#lightboxPressed" prefHeight="390.0" prefWidth="600.0" style="-fx-background-color: white; -fx-background-radius: 25;" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="0.0">
         <bottom>
            <VBox alignment="TOP_CENTER" prefHeight="90.0" prefWidth="600.0" BorderPane.alignment="TOP_CENTER">
               <children>
                  <Button fx:id="closeButton" mnemonicParsing="false" onAction="#closeButtonPressed" styleClass="closeButton" text="Stäng" />
                  <HBox alignment="CENTER">
                     <children>
                        <Button fx:id="backButton" mnemonicParsing="false" onAction="#loadPreviousShoppingPanel" styleClass="backButton" text=" Tillbaka">
                           <HBox.margin>
                              <Insets right="100.0" />
                           </HBox.margin>
                           <graphic>
                              <ImageView fitHeight="17.0" fitWidth="17.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../icons/chevron_left_grey.png" />
                                 </image>
                              </ImageView>
                           </graphic></Button>
                        <HBox alignment="CENTER" spacing="10.0">
                           <children>
                              <Circle fx:id="shoppingPaneCircleGuide1" fill="LIGHTGRAY" radius="8.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" styleClass="dotsUnmarked" />
                              <Circle fx:id="shoppingPaneCircleGuide2" fill="LIGHTGRAY" layoutX="20.0" layoutY="23.0" radius="8.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" styleClass="dotsUnmarked" />
                              <Circle fx:id="shoppingPaneCircleGuide3" fill="LIGHTGRAY" layoutX="50.0" layoutY="23.0" radius="8.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" styleClass="dotsUnmarked" />
                              <Circle fx:id="shoppingPaneCircleGuide4" fill="LIGHTGRAY" layoutX="80.0" layoutY="23.0" radius="8.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" styleClass="dotsUnmarked" />
                           </children>
                        </HBox>
                        <Button fx:id="nextButton" contentDisplay="RIGHT" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#loadNextShoppingPanel" styleClass="nextButton" text="Gå vidare ">
                           <HBox.margin>
                              <Insets left="100.0" />
                           </HBox.margin>
                           <graphic>
                              <ImageView fx:id="blueChevron" fitHeight="17.0" fitWidth="17.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../icons/chevron_right_blue.png" />
                                 </image>
                              </ImageView>
                           </graphic></Button>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </bottom>
         <top>
            <HBox alignment="TOP_CENTER" prefHeight="70.0" prefWidth="600.0" spacing="240.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="pageTitle" prefHeight="36.0" prefWidth="260.0" text="Leveransalternativ">
                     <font>
                        <Font name="Lato Bold" size="28.0" />
                     </font>
                  </Label>
                  <Button alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#closeButtonPressed" prefHeight="35.0" prefWidth="35.0" styleClass="cornerCloseButton" textAlignment="CENTER">
                     <graphic>
                        <ImageView fitHeight="22.0" fitWidth="22.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../icons/close.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
               </children>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
               <padding>
                  <Insets top="20.0" />
               </padding>
            </HBox>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
